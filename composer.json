{
  "name": "xesau/psr7-http-session",
  "description": "PSR-7 and PSR-15 alternative to PHP's native session handler",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "keywords": [
    "psr-7",
    "psr7",
    "session",
    "http",
    "http-session"
  ],
  "authors": [
    {
      "name": "Ujjwal Ojha",
      "email": "bring2uo@gmail.com"
    },
    {
      "name": "Aron van Willigen",
      "email": "me@xesau.eu"
    }
  ],
  "require": {
    "php": ">=7.1",
    "paragonie/cookie": "^3.2.0",
    "symfony/options-resolver": "^3.3",
    "zendframework/zend-math": "^3.0",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^0.9.9"
  },
  "autoload": {
    "psr-4": {
      "Ojhaujjwal\\Session\\": "src/",
      "Ojhaujjwal\\SessionTest\\": "tests/"
    }
  }
}
